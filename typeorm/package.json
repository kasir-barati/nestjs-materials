{
    "name": "typeorm",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier -w . -u",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "./run-integration-tests.sh",
        "openapi:generate": "openapi-generator-cli generate -i ./openApi.json -o api-client -g typescript-axios --additional-properties=useSingleRequestParameter=true",
        "openapi:create": "ts-node --project tsconfig.json --transpile-only -r tsconfig-paths/register src/utils/create-openapi-json.util.ts"
    },
    "dependencies": {
        "@nestjs/common": "^10.2.10",
        "@nestjs/core": "^10.2.10",
        "@nestjs/mapped-types": "^2.0.4",
        "@nestjs/platform-express": "^10.2.10",
        "@nestjs/swagger": "^7.1.16",
        "@nestjs/typeorm": "^10.0.1",
        "pg": "^8.11.3",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.17"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.2.1",
        "@nestjs/schematics": "^10.0.3",
        "@nestjs/testing": "^10.2.10",
        "@openapitools/openapi-generator-cli": "^2.7.0",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.10",
        "@types/node": "^20.10.3",
        "@types/sinon": "^17.0.2",
        "@types/supertest": "^2.0.16",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "eslint": "^8.55.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.0.1",
        "jest": "^29.7.0",
        "prettier": "^3.1.0",
        "sinon": "^17.0.1",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
