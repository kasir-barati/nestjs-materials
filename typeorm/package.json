{
    "name": "typeorm",
    "engines": {
        "node": ">=18"
    },
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier -w . -u",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:integration": "./run-integration-tests.sh",
        "openapi:generate": "openapi-generator-cli generate -i /local/openApi.json -o /local/src/api-client -g typescript-axios --additional-properties=useSingleRequestParameter=true",
        "openapi:create": "ts-node --project tsconfig.json --transpile-only -r tsconfig-paths/register src/utils/create-openapi-json.util.ts"
    },
    "dependencies": {
        "@nestjs/common": "^10.3.0",
        "@nestjs/core": "^10.3.0",
        "@nestjs/mapped-types": "^2.0.4",
        "@nestjs/platform-express": "^10.3.0",
        "@nestjs/swagger": "^7.2.0",
        "@nestjs/typeorm": "^10.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "pg": "^8.11.3",
        "reflect-metadata": "^0.1.14",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.19"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.3.0",
        "@nestjs/schematics": "^10.1.0",
        "@nestjs/testing": "^10.3.0",
        "@openapitools/openapi-generator-cli": "^2.8.0",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.11.5",
        "@types/sinon": "^17.0.3",
        "@types/supertest": "^2.0.16",
        "@typescript-eslint/eslint-plugin": "^6.19.0",
        "@typescript-eslint/parser": "^6.19.0",
        "axios": "^1.6.5",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "jest": "^29.7.0",
        "prettier": "^3.2.4",
        "sinon": "^17.0.1",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.4",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3"
